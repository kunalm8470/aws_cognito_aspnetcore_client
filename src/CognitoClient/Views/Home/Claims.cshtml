@using System.Security.Claims
@{
    ViewData["Title"] = "Claims";
}

<h3>Access Token -</h3>
<textarea>@ViewBag.AccessToken</textarea>

<h3>Id Token -</h3>
<textarea>@ViewBag.IdToken</textarea>

<h3>Claims -</h3>
<table class="table table-striped table-bordered">

    @foreach (Claim claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>
