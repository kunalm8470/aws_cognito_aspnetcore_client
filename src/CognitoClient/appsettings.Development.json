{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cognito": {
    "ClientId": "<AWS_COGNITO_CLIENT_ID>",
    "ClientSecret": "<AWS_COGNITO_CLIENT_SECRET>",
    "MetadataAddress": "https://cognito-idp.<AWS_REGION>.amazonaws.com/<AWS_COGNITO_USER_POOL_ID>/.well-known/openid-configuration",
    "ResponseType": "code",
    "AppSignOutUrl": "/",
    "Domain": "<AWS_COGNITO_USER_POOL_DOMAIN>"
  }
}
